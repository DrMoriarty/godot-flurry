{
  "name": "flurry",
  "display_name": "Flurry",
  "description": "Flurry Analytics plugin for Godot engine",
  "version": "0.1.2",
  "godot_version": "3.3.0",
  "license": "MIT",
  "url": "https://github.com/DrMoriarty/godot-flurry.git",
  "category": "misc",
  "author": {
    "name": "DrMoriarty",
    "url": "https://github.com/DrMoriarty",
    "donate": "https://ko-fi.com/drmoriarty"
  },
  "files": {
    "scripts": "scripts"
  },
  "variables": {
    "Flurry/API_KEY": {
      "info": "Set your Api Key here"
    }
  },
  "autoload": {
    "flurry": "*res://scripts/flurry.gd"
  },
  "platform_ios": {
    "dependencies": [
      "nativelib-export"
    ],
    "files": {
      "ios-framework/build/flurry.gdnlib": "addons/flurry-ios/",
      "ios-framework/build/flurry.gdns": "addons/flurry-ios/",
      "ios-framework/build/.gitignore": "addons/flurry-ios/",
      "ios-framework/build/flurry.xcframework": "addons/flurry-ios/flurry.xcframework"
    }
  },
  "platform_android": {
    "files": {
      "android-plugin/Flurry.gdap": "android/plugins/",
      "android-plugin/build/outputs/aar/Flurry.release.aar": "android/plugins/"
    }
  }
}
